		<x-style3d property='tube' value='.4'></x-style3d>
		<x-style3d property='segments-radial' value='16'></x-style3d>
		<x-style3d property='segments-tubular' value='128'></x-style3d>
	</x-class3d>
	<x-class3d id='material'>
